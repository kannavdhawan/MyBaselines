    "def data_load(data_path):\n",
    "    \"\"\"\n",
    "    Loading preprocessed dataset \n",
    "    \"\"\"\n",
    "    print(\"Loading dataset..\")\n",
    "    dataset=pd.read_csv(data_path,index_col=[0])\n",
    "    X,y=dataset.iloc[:,:-1],dataset['Target']       #separating data and labels\n",
    "    return X,y"
   ]
